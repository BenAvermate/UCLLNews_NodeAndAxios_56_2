{
  "name": "nodeAndAxios",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "startaxios": "nodemon axiosconsole.ts",
    "start": "nodemon app.ts --delay 3"
  },
  "nodemonConfig": {
    "ext": "ts",
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "dependencies": {
    "axios": "^0.25.0",
    "node-json2html": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
